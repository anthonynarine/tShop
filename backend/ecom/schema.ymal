components:
  schemas:
    Category:
      type: object
      properties:
        name:
          type: string
          maxLength: 50
        description:
          type: string
          maxLength: 250
      required:
      - description
      - name
    PatchedCategory:
      type: object
      properties:
        name:
          type: string
          maxLength: 50
        description:
          type: string
          maxLength: 250
    PatchedProduct:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        image:
          type: string
          format: uri
          nullable: true
        name:
          type: string
          maxLength: 50
        description:
          type: string
          maxLength: 250
        price:
          type: integer
        stock:
          type: integer
        category:
          type: integer
          nullable: true
    Product:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        image:
          type: string
          format: uri
          nullable: true
        name:
          type: string
          maxLength: 50
        description:
          type: string
          maxLength: 250
        price:
          type: integer
        stock:
          type: integer
        category:
          type: integer
          nullable: true
      required:
      - description
      - id
      - name
      - price
      - stock
  securitySchemes:
    basicAuth:
      type: http
      scheme: basic
    cookieAuth:
      type: apiKey
      in: cookie
      name: sessionid
    tokenAuth:
      type: apiKey
      in: header
      name: Authorization
      description: Token-based authentication with required prefix "Token"
